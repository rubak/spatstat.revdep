importFrom("graphics", "abline", "barplot", "layout", "lines", "par",
             "plot", "plot.default", "points", "polygon", "symbols",
             "text")
importFrom("stats", "as.dist", "var")
importFrom("utils", "read.table", "str")

# Export all names (should be improved in the future)
